<!-- PUT LINK TO CURRENT USERS PARTNER IF ANY -->
<li><%= link_to "Courses", courses_path, :class => "btn-btn-navbar" %></li>
<% if current_user.is_partner? %>
  <li><%= link_to "Students", students_path, :class => "btn-btn-navbar" %></li>
  <li><%= link_to "My Company", partner_path(current_user.partner) %></li>
<% end %>
<% if current_user.is_student? %>
  <li><%= link_to "Partners", partners_path, :class => "btn-btn-navbar" %></li>
  <li><%= link_to "My Profile", student_path(current_user.student) %></li>
<% end %>
